            <Label style="-fx-font-family: Arial; -fx-font-size: 18;" text="Support beziehen" BorderPane.alignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
               <padding>
                  <Insets top="15.0" />
               </padding>
            </Label>
         </top>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" />
         </padding>
         <center>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="15.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="lblConnectedTo" text="Connected to XY" />
                  <Button mnemonicParsing="false" onAction="#onDisconnectAction" text="Disconnect" />
               </children>
            </VBox>
         </center>
